        <obo:IAO_0000115 xml:lang="en">Cooked at moderate to high levels of heat in which no liquid is added and only small amounts of fat may be added to prevent sticking.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/foodon.owl"/>
