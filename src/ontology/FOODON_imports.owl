        <obo:hasDbXref>LANGUAL:G0005</obo:hasDbXref>
        <obo:hasSynonym xml:lang="en">roasted or baked</obo:hasSynonym>
        <rdfs:label xml:lang="en">baked or roasted</rdfs:label>
    </owl:Class>
    


